{
  "name": "webhooks",
  "version": "1.0.0",
  "author": "Carlos Daniel",
  "scripts": {
    "start": "concurrently yarn:startup-*",
    "startup-server": "sls offline",
    "startup-dynalite": "./node_modules/.bin/dynalite --createTableMs=0 --deleteTableMs=0 --updateTableMs=0 --port=8000",
    "startup-dynamodb-admin": "dynamodb-admin",
    "deploy:run": "serverless deploy",
    "deploy:aws-config": "serverless config credentials -p aws -k $AWS_ACCESS_KEY_ID -s $AWS_SECRET_ACCESS_KEY",
    "lint": "./node_modules/.bin/eslint"
  },
  "dependencies": {
    "axios": "^0.20.0",
    "dynamoose": "^2.7.3",
    "serverless": "^2.53.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "concurrently": "^6.2.0",
    "dynalite": "^3.2.1",
    "dynamodb-admin": "^4.0.3",
    "eslint": "^7.17.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "serverless-bundle": "3.2.0",
    "serverless-dotenv-plugin": "^2.1.1",
    "serverless-offline": "^8.0.0"
  }
}
